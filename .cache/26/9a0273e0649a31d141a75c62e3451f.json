  }} className='button'  onClick = {()=>{\n                        console.log('deleting this ', link.id)\n          \n                        deleteLink(link.id)\n          \n                      } }>Delete</button>\n                    <button onClick = {()=>{\n                        console.log('tis id is for editing', link.id)\n                        setEdit(link.id) }}  className='button' >Update</button>\n                </td>\n            </tr>\n        )\n    })\n  }\nexport default TableRows"},"lineCount":null}},"error":null,"hash":"35df11d4f2e93b00f81db1cece05fc9a","cacheData":{"env":{}}}