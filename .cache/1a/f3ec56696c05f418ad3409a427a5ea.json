tate(\"\");\n  const [comment, setComment] = useState(\"\");\n  const [tag, setTag] = useState(\"\");\n  useEffect(() => {\n    setLink(props.link || \"\");\n    setComment(props.comment || \"\");\n    setTag(props.tag || \"\");\n  }, [linkId]);\n\n  const submitHandler = () =>{\n    createLink({\n        link,\n        comment,\n        tag\n    });\n\n}\n\nconst editHandler = () => {\n\n    editLink({\n        link, \n        comment\n    }, linkId)\n\n}\n\n   \n\n\n\n  \n\n  return (\n    <div>\n      <form\n        className=\"createLinkForm\"\n        onSubmit={async (event) => {\n          event.preventDefault();\n          \n        \n        }}\n      >\n        <label>URL</label>\n        <TextField\n          type=\"text\"\n          value={link}\n          placeholder=\"Link here\"\n          onChange={(event) => {\n            setLink(event.target.value);\n          }}\n        />\n        <label>Comment</label>\n        <TextField\n          type=\"text\"\n          value={comment}\n          placeholder=\"Comment here\"\n          onChange={(event) => {\n            setComment(event.target.value);\n          }}\n        />\n          <label>Tags</label>\n        <TextField\n          type=\"text\"\n          value={tag}\n          placeholder=\"Comment here\"\n          onChange={(event) => {\n            setTag(event.target.value);\n          }}\n        />\n        <div>\n        {linkId ? <button onClick={()=>editHandler()}>Update</button> : <button onClick={()=>{\n            submitHandler()\n          }}>Submit</button>}\n        </div>\n      </form>\n    </div>\n  );\n};\nexport default CreateNewLink;\n"},"lineCount":null}},"error":null,"hash":"ea9e3bbdb6e5fc7d294d40535f1cfd89","cacheData":{"env":{}}}